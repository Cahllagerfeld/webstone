import type { RequestHandler } from '@sveltejs/kit';

// GET {{apiPath}}
export const get: RequestHandler = async (event) => {
  return {
    status: 200,
    body: `${event.request.method.toUpperCase()} ${event.url.pathname} => Ok.`
  };
};

// POST {{apiPath}}
export const post: RequestHandler = async (event) => {
  return {
    status: 200,
    body: `${event.request.method.toUpperCase()} ${event.url.pathname} => Ok.`
  };
};
